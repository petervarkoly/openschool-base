configdirectory: /var/lib/imap
partition-default: /var/spool/imap
sievedir: /var/lib/sieve
admins: cyrus
allowanonymouslogin: no
autocreatequota: 10240000
#autocreateinboxfolders: Spam|Sent|Templates|Trash
#autosubscribeinboxfolders: Spam|Sent|Templates|Trash
#autosubscribe_all_sharedfolders: 1
reject8bit: no
quotawarn: 90
timeout: 120
poptimeout: 30
dracinterval: 0
drachost: localhost
sasl_pwcheck_method: saslauthd
lmtp_overquota_perm_failure: no
altnamespace: yes
unixhierarchysep: yes
allowplainwithouttls: yes
allowplaintext: yes
tls_cert_file: /etc/ssl/servercerts/servercert.pem
tls_key_file: /etc/ssl/servercerts/serverkey.pem
tls_ca_path: /etc/ssl/certs
